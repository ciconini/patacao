{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1"
  },
  "testTimeout": 60000,
  "setupFilesAfterEnv": ["<rootDir>/setup-e2e-tests.ts"],
  "globalSetup": "<rootDir>/global-setup-e2e.ts",
  "globalTeardown": "<rootDir>/global-teardown-e2e.ts"
}

